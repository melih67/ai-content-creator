# Supabase Configuration
VITE_SUPABASE_URL=your-supabase-project-url
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# API Configuration (fallback for local development)
VITE_API_BASE_URL=http://localhost:3001/api

# JWT Secret (for development only)
JWT_SECRET=your-secret-key-here

# Development Settings
VITE_APP_NAME=AIVA
VITE_APP_VERSION=1.0.0

# Authentication
VITE_JWT_SECRET=your-jwt-secret-key
VITE_JWT_EXPIRES_IN=7d
VITE_REFRESH_TOKEN_EXPIRES_IN=30d

# AI Services
VITE_OPENAI_API_KEY=your-openai-api-key
VITE_OPENAI_MODEL=gpt-4
VITE_OPENAI_MAX_TOKENS=2000

# Social Media APIs
VITE_FACEBOOK_APP_ID=your-facebook-app-id
VITE_FACEBOOK_APP_SECRET=your-facebook-app-secret
VITE_INSTAGRAM_CLIENT_ID=your-instagram-client-id
VITE_INSTAGRAM_CLIENT_SECRET=your-instagram-client-secret
VITE_TWITTER_API_KEY=your-twitter-api-key
VITE_TWITTER_API_SECRET=your-twitter-api-secret
VITE_LINKEDIN_CLIENT_ID=your-linkedin-client-id
VITE_LINKEDIN_CLIENT_SECRET=your-linkedin-client-secret
VITE_TIKTOK_CLIENT_KEY=your-tiktok-client-key
VITE_TIKTOK_CLIENT_SECRET=your-tiktok-client-secret

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,image/webp,video/mp4,video/webm

# Analytics
VITE_GOOGLE_ANALYTICS_ID=your-google-analytics-id
VITE_MIXPANEL_TOKEN=your-mixpanel-token

# Payment
VITE_STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key
VITE_STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret

# Email
VITE_SENDGRID_API_KEY=your-sendgrid-api-key
VITE_FROM_EMAIL=noreply@aiva.ai

# Storage
VITE_AWS_ACCESS_KEY_ID=your-aws-access-key
VITE_AWS_SECRET_ACCESS_KEY=your-aws-secret-key
VITE_AWS_REGION=us-east-1
VITE_AWS_S3_BUCKET=aiva-uploads

# Redis (for caching and sessions)
VITE_REDIS_URL=redis://localhost:6379

# Database
VITE_DATABASE_URL=postgresql://username:password@localhost:5432/aiva_db

# Development
VITE_DEBUG=true
VITE_LOG_LEVEL=debug

# Production
VITE_SENTRY_DSN=your-sentry-dsn
VITE_APP_URL=https://aiva.ai